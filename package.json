    "axios": "^0.21.0",
    "clipboard-polyfill": "^3.0.1",
    "clsx": "^1.1.1",
    "geojson": "^0.5.0",
    "googleapis": "^67.0.0",
    "joi": "^17.3.0",
    "leaflet": "file:./packages/leaflet",
    "leaflet-draw": "^1.0.4",
    "leaflet-editable": "^1.2.0",
    "leaflet-textpath": "file:./packages/leaflet.textpath",
    "leaflet.markercluster": "file:./packages/leaflet.markercluster",
    "lodash": "^4.17.21",
    "new-github-issue-url": "^0.2.1",
    "next": "latest",
    "notistack": "^1.0.3",
    "object-hash": "^2.0.3",
    "react": "latest",
    "react-adaptive-hooks": "^0.0.8",
    "react-dom": "latest",
    "react-dropzone": "^11.2.4",
    "react-leaflet": "file:./packages/react-leaflet/packages/react-leaflet/",
    "react-leaflet-cluster": "^1.0.2",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "react-leaflet-textpath": "file:./packages/react-leaflet-textpath",
    "react-localization": "file:./packages/react-localization",
    "react-redux": "^7.2.2",
    "react-swipeable-views": "^0.13.9",
    "react-tooltip": "^4.2.10",
    "react-world-flags": "^1.4.0",
    "redux": "^4.0.5",
    "sanitize-html": "^2.1.2",
    "sourcemapped-stacktrace": "^1.1.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.20",
    "@types/leaflet": "^1.7.0",
    "@types/leaflet-textpath": "^1.2.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/object-hash": "^1.3.4",
    "@types/react": "^17.0.1",
    "@types/react-adaptive-hooks": "^0.0.0",
    "@types/react-dom": "^17.0.2",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/react-leaflet-markercluster": "^2.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-world-flags": "^1.4.1",
    "@types/sanitize-html": "^1.27.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-plugin-root-import": "^6.6.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "file:./packages/next-transpile-modules",
    "prettier": "^2.2.1",
    "react-dev-utils": "^11.0.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "serve": "^11.3.2",
    "ts-toolbelt": "^9.5.10",
    "type-coverage": "^2.16.3",
    "typescript": "^4.1.5",
    "typescript-coverage-report": "^0.5.0",
    "url-loader": "^4.1.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "resolutions": {
    "@react-leaflet/core": "file:./packages/react-leaflet/packages/core",
    "react-leaflet": "file:./packages/react-leaflet/packages/react-leaflet",
    "react-leaflet-textpath": "file:./packages/react-leaflet-textpath",
    "react-localization": "file:./packages/react-localization",
    "leaflet.markercluster": "file:./packages/leaflet.markercluster",
    "leaflet-textpath": "file:./packages/leaflet.textpath",
    "leaflet": "file:./packages/leaflet",
    "next-transpile-modules": "file:./packages/next-transpile-modules",
    "webpack": "^5.30.0"
  },
  "typeCoverage": {
    "atLeast": 95,
    "strict": true,
    "ignoreFiles": [
      "packages/**/*"
    ],
    "outputDir": "build/type-coverage"
  }
}
