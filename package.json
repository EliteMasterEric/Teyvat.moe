{
  "name": "genshin-map",
  "version": "0.12.1-alpha",
  "license": "GPL-3.0",
  "homepage": "https://genshinmap.github.io/",
  "bugs": {
    "url": "https://github.com/GenshinMap/genshinmap.github.io/issues"
  },
  "scripts": {
    "preinstall": "yarn policies set-version 1.22.10",
    "install:dev": "yarn install --prod=false",
    "install:prod": "yarn install --prod=true",
    "install:check": "yarn install --check-files --prod=false",
    "start:dev": "npx next dev",
    "start:prod": "npx next start",
    "build": "npx next build",
    "export": "npx next export",
    "predeploy": "npm run build",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "eslint src --ext .ts,.tsx --ignore-pattern node_modules/",
    "lint:md": "remark .",
    "type-check": "tsc --pretty --noEmit",
    "type-coverage": "typescript-coverage-report",
    "test": "echo \"This test is blank. Do not remove it, as it is required by TravisCI.\""
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-undefined-references",
        false
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@react-leaflet/core": "file:./packages/react-leaflet/packages/core/",
